{
  "name": "mmikulski/wp-docker",
  "description": "website",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Maciej Mikulski",
      "email": "maciej.mikulski@sparklab.pl"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "extra": {
    "wordpress-install-dir": "wordpress",
    "installer-paths": {
      "wordpress/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "wordpress/wp-content/themes/{$name}/": ["type:wordpress-theme"]
    }
  },
  "require": {
    "johnpbloch/wordpress": "^5.3",
    "wpackagist-theme/twentytwenty": "^1.1",
    "wpackagist-plugin/akismet": "^4.1"
  },
  "require-dev": {
    "wp-cli/wp-cli": "^2.4"
  }
}
